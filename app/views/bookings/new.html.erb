<%= simple_form_for [@castle ,@booking] do |f| %>
  <%= f.error_notification %>
  <%= f.input :date_begin,as: :string,
            input_html: { data: { controller: "datepicker" } } %>
  <%= f.input :date_end,as: :string,
            input_html: { data: { controller: "datepicker" } } %>
  <%= f.submit %>
<% end %>
